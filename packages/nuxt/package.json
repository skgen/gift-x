{
  "name": "skgen.me",
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "prepare": "nuxt prepare",
    "review": "pnpm review:typescript && pnpm review:eslint && pnpm review:stylelint",
    "review:typescript": "vue-tsc --noEmit -p tsconfig.app.json",
    "review:eslint": "eslint . --ext .vue,.ts,.tsx,.cts,.mts --fix --ignore-pattern .gitignore",
    "review:stylelint": "stylelint \"{app,layers}/**/*.{scss,vue}\" --fix --color",
    "sdk": "concurrently --names \"GRAPHQL,REST\" --prefix-colors \"cyan,yellow\" \"pnpm run sdk:graphql\" \"pnpm run sdk:rest\"",
    "sdk:rest": "tsx --tsconfig tsconfig.scripts.json scripts/rest.sdk.ts",
    "sdk:graphql": "tsx --tsconfig tsconfig.scripts.json scripts/graphql.sdk.ts"
  },
  "dependencies": {
    "@atmina/urql-custom-scalars-exchange": "catalog:runtime",
